<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="514.0" prefWidth="612.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ynr.prison.controller.ControllerVisites">
   <children>
      <HBox prefHeight="90.0" prefWidth="612.0">
         <children>
            <Label prefHeight="24.0" prefWidth="99.0" text="Matricule :" textAlignment="JUSTIFY">
               <HBox.margin>
                  <Insets left="10.0" top="4.0" />
               </HBox.margin>
               <padding>
                  <Insets left="30.0" />
               </padding>
            </Label>
            <TextField fx:id="prenom" prefHeight="25.0" prefWidth="158.0">
               <HBox.margin>
                  <Insets top="4.0" />
               </HBox.margin>
            </TextField>
            <Button alignment="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="156.0" text="Rechercher" textAlignment="JUSTIFY">
               <HBox.margin>
                  <Insets left="15.0" top="4.0" />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets left="20.0" top="20.0" />
         </padding>
      </HBox>
      <TableView prefHeight="391.0" prefWidth="612.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Nom" />
          <TableColumn prefWidth="75.0" text="Prénom" />
            <TableColumn prefWidth="97.0" text="Lien de Parenté" />
            <TableColumn prefWidth="75.0" text="CIN" />
            <TableColumn prefWidth="63.0" text="Date" />
            <TableColumn prefWidth="76.0" text="Heure" />
            <TableColumn prefWidth="129.0" text="Nombre de Visiteurs" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <VBox alignment="CENTER" prefHeight="66.0" prefWidth="612.0">
         <children>
            <Button fx:id="ajouterVisiteButton" mnemonicParsing="false" onAction="#ajouterVisite" prefHeight="28.0" prefWidth="98.0" text="Ajouter Visite" />
         </children>
      </VBox>
   </children>
</VBox>
